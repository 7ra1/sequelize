{
  "bugs": "https://github.com/sequelize/sequelize/issues",
  "description": "Turso Connector for Sequelize, based on the @tursodatabase/database npm package",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.mts",
        "default": "./lib/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    }
  },
  "files": [
    "lib"
  ],
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "homepage": "https://sequelize.org",
  "license": "MIT",
  "name": "@sequelize/turso",
  "repository": "https://github.com/sequelize/sequelize",
  "scripts": {
    "build": "../../build-packages.mjs turso",
    "test": "concurrently \"npm:test-*\"",
    "test-typings": "tsc --noEmit --project tsconfig.json",
    "test-exports": "../../dev/sync-exports.mjs ./src --check-outdated",
    "sync-exports": "../../dev/sync-exports.mjs ./src"
  },
  "type": "commonjs",
  "version": "7.0.0-alpha.47",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@sequelize/core": "workspace:*",
    "@sequelize/utils": "workspace:*",
    "@tursodatabase/database": "^0.4.0-pre.1",
    "lodash": "^4.17.21"
  }
}
